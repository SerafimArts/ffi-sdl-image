{
    "name": "serafim/ffi-sdl-image",
    "description": "PHP FFI to SDL Image bindings",
    "type": "library",
    "license": "MIT",
    "support": {
        "issues": "https://github.com/SerafimArts/ffi-sdl-image/issues",
        "source": "https://github.com/SerafimArts/ffi-sdl-image"
    },
    "repositories": [
        {
            "type": "path",
            "url": "../SDL"
        }
    ],
    "keywords": [
        "sdl",
        "ffi",
        "bindings",
        "opengl",
        "graphics",
        "library",
        "image"
    ],
    "homepage": "https://github.com/SerafimArts/ffi-sdl-image",
    "authors": [
        {
            "name": "Kirill Nesmeyanov",
            "email": "nesk@xakep.ru"
        }
    ],
    "require": {
        "php": ">=7.4",
        "ext-ffi": "*",
        "serafim/ffi-sdl": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "SDL\\Image\\": "src"
        }
    },
    "require-dev": {
        "serafim/ffi-ide-helper": "^1.0",
        "phpunit/phpunit": "~9.0",
        "symfony/var-dumper": "~5.0"
    },
    "autoload-dev": {
        "psr-4": {
            "SDL\\Image\\Tests\\": "tests"
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    }
}
